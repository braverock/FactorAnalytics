    leaps,
    lars,
    strucchange,
    lmtest,
    sandwich,
    lattice,
    MASS,
    sn,
    boot,
    parallel,
    doSNOW,
    RCurl,
    bestglm,
    tseries
Suggests:
    testthat
LazyLoad: yes
LazyDataCompression: xz
URL: http://r-forge.r-project.org/projects/returnanalytics/
